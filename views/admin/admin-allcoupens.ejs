<body>
    <div class="screen-overlay"></div>



    <section class="content-main">
        <div class="content-header">
            <div>
                <h2 class="content-title card-title">Order List </h2>
                <p>Lorem ipsum dolor sit amet.</p>
            </div>
            <div>
                <a href="/admin/admin-addcoupoen"> <button class="btn btn-md rounded font-sm ">Add Coupon </button></a>
            </div>
        </div>
        <div class="card mb-4">
            <header class="card-header">
                <div class="row gx-3">
                    <div class="col-lg-4 col-md-6 me-auto">

                    </div>
                    <div class="col-lg-2 col-6 col-md-3">
                        <select class="form-select">
                            <option>Status</option>
                            <option>Active</option>
                            <option>Disabled</option>
                            <option>Show all</option>
                        </select>
                    </div>
                    <div class="col-lg-2 col-6 col-md-3">
                        <select class="form-select">
                            <option>Show 20</option>
                            <option>Show 30</option>
                            <option>Show 40</option>
                        </select>
                    </div>
                </div>
            </header> <!-- card-header end// -->
            <div class="card-body">
                <div class="table-responsive">
                    <table class="table table-hover">
                        <thead>
                            <tr>
                                <th>#ID</th>
                                <th scope="col">Name</th>
                                <th scope="col">Code</th>
                                <th scope="col">Validity</th>
                                <th scope="col">Max Discound</th>
                                <th scope="col">Mim Purchase</th>
                                <th scope="col">Status</th>
                                <th scope="col" class="text-end"> Action </th>
                            </tr>
                        </thead>


                        <tbody>

                            <% coupens.forEach((coupen,index)=>{%>
                                
                                    <tr>
                                        <td>
                                            <%= index+1 %>
                                        </td>
                                        <td><b>
                                                <%= coupen.name %>
                                            </b></td>
                                        <td>
                                            <b>
                                                <%= coupen.code %>
                                            </b>
                                        </td>
                                        <td><b>
                                                <%= coupen.validity.toLocaleDateString() %>
                                            </b></td>
                                        <td>
                                            <%= coupen.maxAmount %>
                                        </td>
                                        <td>
                                            <%= coupen.minAmount %>
                                        </td>

                                        <td>
                                            <% if(coupen.status){%>
                                                <span class="badge rounded-pill alert-success">Active</span>

                                                <%}else{%>
                                                    <span class="badge rounded-pill alert-danger">Disabled</span>

                                                    <%}%>
                                        </td>
                                        <td class="text-end">
                                            <% if(coupen.status){%>
                                                <a href="/admin/coupenstatus/<%= coupen._id %>"
                                                    class="btn btn-md-danger btn-danger rounded font-sm">Disable</a>
                                                <%}else{%>
                                                    <a href="/admin/coupenstatus/<%= coupen._id %>"
                                                        class="btn btn-md rounded font-sm">Active</a>
                                                    <%}%>
                                        </td>
                                    </tr>
                                    
                                        <%})%>
                        </tbody>
                    </table>
                </div> <!-- table-responsive //end -->
            </div> <!-- card-body end// -->
        </div> <!-- card end// -->
